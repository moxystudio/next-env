{
  "name": "@moxy/next-process-env",
  "version": "2.0.1",
  "description": "Next.js plugin to pass environment variables to Next's configuration",
  "author": "Ivo Lima Silva <ivolimasilva@outlook.com>",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "next",
    "nextjs",
    "plugins",
    "environment",
    "env",
    "dotenv",
    "runtime",
    "buildtime"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/next-process-env.git"
  },
  "scripts": {
    "build": "BABEL_ENV=commonjs babel src -d lib --delete-dir-on-start",
    "lint": "eslint . --ignore-path .gitignore",
    "test": "jest",
    "prerelease": "npm t && npm run lint && npm run build",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@moxy/babel-preset": "^3.2.1",
    "eslint": "^5.16.0",
    "eslint-config-moxy": "^8.1.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "standard-version": "^7.0.0"
  },
  "peerDependencies": {
    "next": ">=8.0.0 <=9.0.0"
  },
  "dependencies": {
    "require-poisoning": "^2.0.0"
  }
}
